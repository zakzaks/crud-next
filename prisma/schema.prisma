generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Plants{
  id String @id @default(cuid())
  name String
  description String?
  category String
  stock Int
  price Float
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
  userId String
  imageUrl String?
}
